<!-- configuration for trends -->  

    <record model="ir.ui.view" id="trends_config_tree_view">
        <field name="name">trends.config.tree</field>
        <field name="model">trends.config</field>
        <field name="arch" type="xml">
            <tree string="Trends Configuration">
                <field name="name"/>  
                <field name="api_key"/> 
                <field name="language"/> 
                <field name="url"/> 
                <field name="type"/>        
            </tree>
        </field>
    </record>
    
    <record model="ir.ui.view" id="trends_config_form_view">
        <field name="name">trends.config.form</field>
        <field name="model">trends.config</field>
        <field name="arch" type="xml">
            <form string="Trends Configuration" version="7.0">
                <header>
                    <button name="action_load_sectorlist" string="Load Sector List" type="object" icon="gtk-execute"/>         
                    <button name="action_load_nacebellist" string="Load Nacebel List" type="object" icon="gtk-execute"/>      
                    <button name="action_load_figurelist" string="Load Figure List" type="object" icon="gtk-execute"/>      
                    <button name="action_load_functionlist" string="Load Function List" type="object" icon="gtk-execute"/>   
                    <button name="action_update_credit" string="Refresh your credit" type="object" icon="gtk-execute"/>                                                                                      
                </header>                
              <sheet>
                <group>
                <field name="name"/>  
                <field name="pwd" password="1"/> 
                <field name="user_id" required="1"/>   
                <field name="credit" readonly="1"/>
                <field name="save"/>               
                <field name="api_key"/> 
                <field name="language"/> 
                <field name="url"/> 
                <field name="type"/> 
                <field name="states"/>                 
                </group>
              </sheet>
            </form>
         </field>
     </record>
     
     <record id="open_view_trends_config" model="ir.actions.act_window">
         <field name="name">Trends Configuration</field>
         <field name="type">ir.actions.act_window</field>
         <field name="res_model">trends.config</field>
         <field name="view_type">form</field>
         <field name="view_mode">tree,form</field>
         <field name="help" type="html">
           <p class="oe_view_nocontent_create">
             Click to add a new trends configuration
